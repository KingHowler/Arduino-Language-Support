{
  "name": "Arduino-Language-Support",
  "displayName": "Arduino Language Support",
  "description": "Language support extension to allow proper editing and programming of .ino files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "ino",
      "aliases": ["Arduino", "ino"],
      "extensions": [".ino",".h",".cpp"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "ino",
      "scopeName": "source.arduino",
      "path": "./syntaxes/ino.tmLanguage.json"
    }]
  }
}
